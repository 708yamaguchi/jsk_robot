# For fetch PC
[Desktop Entry]
Version=1.0
Type=Application
Terminal=false
Icon=@PROJECT_SOURCE_DIR@/data/fetch.jpg
Name=Fetch RViz
# We use localhost because we can see RViz from the fetch monitor if the network is down
Exec=/bin/bash -l -i -c "DISPLAY=:0.0 rossetip && rossetmaster localhost && rosrun rviz rviz -d $(rospack find jsk_fetch_startup)/config/jsk_startup.rviz"
